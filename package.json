{
  "name": "bitx-express-typescript-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "license": "MIT",
  "scripts": {
    "start": "nodemon",
    "start:prod": "NODE_ENV=production PORT=3000 node dist/server.js",
    "prod": "npx gulp prod",
    "build": "npx gulp build",
    "docs": "apidoc -i src -o public",
    "docs-md": "npx gulp docs:markdown",
    "exec:ts": "ts-node --project tsconfig.json -r tsconfig-paths/register"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "apidoc": {
